proc mdc (n : int, m : int, r : @int) {
	if(m % n == 0) {
		@r = n;
	}
	else {
		if(n % m == 0){
			@r = m;
		}
		else {
			if(m > n){
				mdc(n, m%n, r);
			}
			else{
				mdc(m, n%m, r);
			}
		}
	}	
}

main() {
	m : int;
	n : int;
	r : int;

	write ("Digite dois numeros inteiros: \n");
	m = readInt();
	n = readInt();

	mdc(n,m, $r);
	write ("O mdc de ");
	write (m);
	write (" e ");
	write (n);
	write (" Ã©: ");
	write (r);

	write("\n");
	return 0;
}
